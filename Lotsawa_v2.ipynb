    "strn=re.sub(r'(ལས[༔།]</div>\\s)(.+?[༔།]+)([\\s།]+ཞེས)', '\\\\1<div class=\"cite\">\\\\2</div>\\\\3',strn, 0)\n",
    "#ལས[༔།]</div>\\s(.+?[༔།]+)[\\s།]+ཞེས\n",
