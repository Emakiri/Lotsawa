    "strn=re.sub(f'(ག་གོ|ང་ངོ[་]*|ད་དོ|ན་ནོ|བ་བོ|མ་མོ|ར་རོ|ལ་ལོ|ས་སོ|ད་ཏོ|འོ)([༔།\s\t]+)', r'\1\2<br>',strn)\n",
    "#strn=re.sub(f'([༄༅། ༿༧་|]*\\S+?)({syl})([་༔།༾ཿ |]+)', r\"\\1་\\2\\3\", strn)\n",
    "\n",
    "\n",
    "print(strn)"
